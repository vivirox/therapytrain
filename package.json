    "next-connect": "^1.0.0",
    "next-themes": "0.4.4",
    "openai": "4.25.0",
    "react": "18.2.0",
    "react-day-picker": "8.10.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.49.3",
    "sonner": "1.3.1",
    "tailwind-merge": "2.2.0",
    "tailwindcss-animate": "1.0.7",
    "tsyringe": "^4.8.0",
