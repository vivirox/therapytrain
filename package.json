{
  "name": "gradiant",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": ">=22.0.0 <23.0.0",
    "pnpm": ">=10.0.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "vercel-build": "pnpm install && vite build",
    "lint": "next lint",
    "preview": "vite preview",
    "setup:zk": "pnpm run scripts/setup_zk.ts",
    "setup-zk": "node --experimental-modules scripts/setup_zk.js",
    "test:zk": "pnpm run setup:zk && pnpm test src/test/zk/*.test.ts",
    "tsc": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --config vitest.config.ts",
    "type-check": "tsc --noEmit",
    "ts:check-strict": "tsx scripts/migrate-types.ts",
    "ts:fix": "tsx scripts/fix-types.ts",
    "ts:migrate": "pnpm ts:check-strict && pnpm ts:fix",
    "test:e2e": "pyppeteer test",
    "test:e2e:ui": "pyppeteer test --ui",
    "test:e2e:debug": "pyppeteer test --debug",
    "test:e2e:report": "pyppeteer show-report",
    "build:essentials:force": "pnpm clean:build && turbo build --filter=\"payload...\" --filter=\"@payloadcms/ui\" --filter=\"@payloadcms/next\" --filter=\"@payloadcms/db-postgres\" --filter=\"@payloadcms/richtext-lexical\" --filter=\"@payloadcms/translations\" --filter=\"@payloadcms/plugin-cloud\" --filter=\"@payloadcms/graphql\" --no-cache --force",
    "download:face-models": "tsx scripts/download-face-models.ts",
    "download:mediapipe-models": "tsx scripts/download-mediapipe-models.ts",
    "download:models": "pnpm download:face-models && pnpm download:mediapipe-models"
  },
  "dependencies": {
