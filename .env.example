# Database
POSTGRES_PASSWORD=your-postgres-password
POSTGRES_PRISMA_URL=your-prisma-url
POSTGRES_URL=your-postgres-url
POSTGRES_URL_NON_POOLING=your-non-pooling-url

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project-url.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key
SUPABASE_JWT_SECRET=your-jwt-secret
SUPABASE_KEY=your-supabase-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Redis
UPSTASH_REDIS_REST_URL=your-redis-url
UPSTASH_REDIS_REST_TOKEN=your-redis-token

# Email
RESEND_API_KEY=your-resend-key
EMAIL_FROM=noreply@example.com

# AI Services
ANTHROPIC_API_KEY=your-anthropic-key
OPENAI_API_KEY=your-openai-key
OPENAI_BASE_URL=https://api.openai.com/v1
AZURE_OPENAI_API_KEY=your-azure-openai-key
AZURE_OPENAI_MODEL_DEPLOYMENT=your-azure-deployment-name
AZURE_OPENAI_API_VERSION=2023-12-01-preview
AZURE_OPENAI_API_ENDPOINT=https://your-azure-instance.openai.azure.com
TOGETHER_API_KEY=your-together-key
REPLICATE_API_TOKEN=your-replicate-token

# Security
JWT_SECRET=your-secret-key
ENCRYPTION_KEY=your-encryption-key

# ZK System
ZK_ENABLED=false
ZK_PROVING_KEY_PATH=./keys/proving_key.json
ZK_VERIFICATION_KEY_PATH=./keys/verification_key.json

# Analytics
NEXT_PUBLIC_ANALYTICS_ENABLED=false
AXIOM_DATASET=your-axiom-dataset
AXIOM_TOKEN=your-axiom-token
SENTRY_DSN=your-sentry-dsn

# Rate Limiting
ENABLE_RATE_LIMITING=true
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

# Logging
LOG_LEVEL=debug

# Server
PORT=3000
PYTHON_VERSION=3.10.16

# Vite Configuration
VITE_API_URL=http://localhost:3000/api
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
VITE_SUPABASE_URL=your-supabase-url

# Google
GOOGLE_API_KEY=your-google-api-key

# Vercel
VERCEL_TOKEN=your-vercel-token
VERCEL_ORG_ID=your-org-id
VERCEL_PROJECT_ID=your-project-id